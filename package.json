{
  "name": "licensezero-cli",
  "version": "0.0.0",
  "description": "License Zero command-line interface",
  "author": "Kyle E. Mitchell <kyle@kemitchell.com> (https://kemitchell.com/)",
  "private": true,
  "devDependencies": {
    "husky": "^0.14.3",
    "istanbul": "^0.4.5",
    "licensee": "^4.5.0",
    "lint-staged": "^4.0.4",
    "standard": "^10.0.3",
    "standard-markdown": "^4.0.2",
    "tape": "^4.8.0",
    "uuid": "^3.1.0"
  },
  "dependencies": {
    "ajv": "^5.2.2",
    "docopt": "^0.6.2",
    "ed25519": "0.0.4",
    "json-parse-errback": "^2.0.1",
    "json-preserve-indent": "^1.1.3",
    "lamos": "^3.0.3",
    "mkdirp": "^0.5.1",
    "normalize-package-data": "^2.4.0",
    "opener": "^1.4.3",
    "read": "^1.0.7",
    "read-package-tree": "^5.1.6",
    "rimraf": "^2.6.1",
    "run-parallel": "^1.1.6",
    "run-series": "^1.1.4",
    "simple-concat": "^1.0.0",
    "stream-buffers": "^3.0.1",
    "xdg-basedir": "^3.0.0"
  },
  "scripts": {
    "coverage": "NODE_ENV=test istanbul cover -- tape test/*.test.js",
    "postcoverage": "istanbul check --statements 100 --functions 100 --branches 100 --lines 100",
    "test": "NODE_ENV=test tape test/*.test.js",
    "lint": "standard && standard-markdown",
    "precommit": "lint-staged",
    "licenses": "licensee --errors-only",
    "prepublish": "./create-bin-scripts"
  },
  "lint-staged": {
    "*.js": "standard",
    "*.md": "standard-markdown"
  },
  "bin": {
    "l0-bom": "./bin/l0-bom",
    "l0-import": "./bin/l0-import",
    "l0-create-licensee": "./bin/l0-create-licensee",
    "l0-remove-licensee": "./bin/l0-remove-licensee",
    "l0-show-licensee": "./bin/l0-show-licensee",
    "l0-list-licensees": "./bin/l0-list-licensees",
    "l0-license": "./bin/l0-license",
    "l0-create-licensor": "./bin/l0-create-licensor",
    "l0-list-licensors": "./bin/l0-list-licensors",
    "l0-remove-licensor": "./bin/l0-remove-licensor",
    "l0-offer": "./bin/l0-offer",
    "l0-purchased": "./bin/l0-purchased"
  }
}
