    "tweetnacl": "^1.0.0",
    "update-notifier": "^2.2.0",
    "xdg-basedir": "^3.0.0"
  },
  "scripts": {
    "coverage": "NODE_ENV=test istanbul cover -- tape test/*.test.js",
