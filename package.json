    "l0-offer": "./bin-loader",
    "l0-purchased": "./bin-loader",
    "l0-quote": "./bin-loader",
    "l0-register-licensor": "./bin-loader",
    "l0-remove-licensee": "./bin-loader",
    "l0-remove-licensor": "./bin-loader",
    "l0-reset-token": "./bin-loader",
